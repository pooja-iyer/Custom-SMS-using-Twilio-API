<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_classification__c</fullName>
    <description>Used to display the customer classification. Value decided from - Total_Customer_Spend__c field.</description>
    <externalId>false</externalId>
    <formula>IF(Total_Customer_Spend__c &gt;= 50000, &quot;GOLD&quot;,
IF(Total_Customer_Spend__c &gt;= 25000, &quot;SILVER&quot;,
IF(Total_Customer_Spend__c &gt;= 10000, &quot;BRONZE&quot;, &quot;&quot;) ) )</formula>
    <label>Customer classification</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
